<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
       <!-- <startup> 
            <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
        </startup>!-->

     
    
        <service behaviorConfiguration="Default"
                 name="FindNDriveServices.Services.PrototypeService">
      
          <host>
            <baseAddresses>
              <add baseAddress="http://localhost:8080/prototypeservice"/>
            </baseAddresses>
          </host>

          <endpoint address="" binding="webHttpBinding"
                    contract="FindNDriveServices.Contracts.IPrototypeService"
                    behaviorConfiguration="Web"/>
        </service>
      </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="Default">
            <serviceMetadata httpGetEnabled="true"/>
            <serviceDebug includeExceptionDetailInFaults="true" />
          </behavior>
        </serviceBehaviors>
        <endpointBehaviors>
          <behavior name="Web">
            <webHttp/>
          </behavior>
        </endpointBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>