<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="Default" name="FindNDriveServices.Services.UserService">
        <endpoint address="" behaviorConfiguration="Web" binding="webHttpBinding"
          contract="FindNDriveServices.Contracts.IUserService" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/userservice" />
          </baseAddresses>
        </host>
      </service>
    </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="Default">
            <serviceMetadata httpGetEnabled="true"/>
            <serviceDebug includeExceptionDetailInFaults="true" />
          </behavior>
        </serviceBehaviors>
        <endpointBehaviors>
          <behavior name="Web">
            <webHttp/>
          </behavior>
        </endpointBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>